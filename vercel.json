{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/setup",
      "dest": "/api/setup"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
